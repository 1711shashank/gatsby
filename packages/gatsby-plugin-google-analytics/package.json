{
  "name": "gatsby-plugin-google-analytics",
  "description": "Gatsby plugin to add google analytics onto a site",
  "version": "2.3.14",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@testing-library/react": "^9.5.0",
    "babel-preset-gatsby-package": "^0.5.3",
    "cross-env": "^7.0.2"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-google-analytics#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "google analytics"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^2.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-google-analytics"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "types": "./index.d.ts",
  "gatsbyPlugin": {
    "optionsSchema": {
      "type": "options",
      "properties": {
        "trackingId": {
          "description": "The property ID; the tracking code won't be generated without it",
          "type": "string"
        },
        "head": {
          "description": "Defines where to place the tracking script - `true` in the head and `false` in the body",
          "type": "boolean"
        },
        "anonymize": {
          "description": "Setting this parameter is optional",
          "type": "boolean"
        },
        "respectDNT": {
          "description": "Setting this parameter is also optional",
          "type": "boolean"
        },
        "exclude": {
          "description": "Avoids sending pageview hits from custom paths",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "pageTransitionDelay": {
          "description": "Delays sending pageview hits on route update (in milliseconds)",
          "type": "number"
        },
        "optimizeId": {
          "description": "Enables Google Optimize using your container Id",
          "type": "string"
        },
        "experimentId": {
          "description": "Enables Google Optimize Experiment ID",
          "type": "string"
        },
        "variationId": {
          "description": "Set Variation ID. 0 for original 1,2,3....",
          "type": "string"
        },
        "defer": {
          "description": "Defers execution of google analytics script after page load",
          "type": "boolean"
        },
        "sampleRate": {
          "type": "number"
        },
        "siteSpeedSampleRate": {
          "type": "number"
        },
        "cookieDomain": {
          "type": "string"
        }
      },
      "required": ["trackingId"]
    }
  }
}
